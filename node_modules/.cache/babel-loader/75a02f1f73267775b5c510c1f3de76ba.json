{"ast":null,"code":"import router from '@/router';\nexport default {\n  created() {\n    this.current = this.$store.state.pageState.online;\n    this.listOnlineUsers();\n  },\n\n  data() {\n    return {\n      loading: true,\n      users: [],\n      keywords: null,\n      current: 1,\n      size: 10,\n      count: 0,\n      isCheck: false,\n      optLog: {}\n    };\n  },\n\n  methods: {\n    listOnlineUsers() {\n      this.axios.get('/api/admin/users/online', {\n        params: {\n          current: this.current,\n          size: this.size,\n          keywords: this.keywords\n        }\n      }).then(({\n        data\n      }) => {\n        this.users = data.data.records;\n        this.count = data.data.count;\n        this.loading = false;\n      });\n    },\n\n    sizeChange(size) {\n      this.size = size;\n      this.listOnlineUsers();\n    },\n\n    currentChange(current) {\n      this.current = current;\n      this.$store.commit('updateOnlinePageState', current);\n      this.listOnlineUsers();\n    },\n\n    removeOnlineUser(user) {\n      this.axios.delete('/api/admin/users/' + user.userInfoId + '/online').then(({\n        data\n      }) => {\n        if (data.flag) {\n          this.$notify.success({\n            title: '成功',\n            message: data.message\n          });\n\n          if (user.userInfoId == this.$store.state.userInfo.id) {\n            router.push({\n              path: '/login'\n            });\n            sessionStorage.removeItem('token');\n          }\n\n          this.listOnlineUsers();\n        } else {\n          this.$notify.error({\n            title: '失败',\n            message: data.message\n          });\n        }\n      });\n    }\n\n  },\n  computed: {\n    tagType() {\n      return function (type) {\n        switch (type) {\n          case 'GET':\n            return '';\n\n          case 'POST':\n            return 'success';\n\n          case 'PUT':\n            return 'warning';\n\n          case 'DELETE':\n            return 'danger';\n        }\n      };\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA0DA;AAEA;EACAA;IACA;IACA;EACA,CAJA;;EAKAC;IACA;MACAC,aADA;MAEAC,SAFA;MAGAC,cAHA;MAIAC,UAJA;MAKAC,QALA;MAMAC,QANA;MAOAC,cAPA;MAQAC;IARA;EAUA,CAhBA;;EAiBAC;IACAC;MACA,WACAC,GADA,CACA,yBADA,EACA;QACAC;UACAR,qBADA;UAEAC,eAFA;UAGAF;QAHA;MADA,CADA,EAQAU,IARA,CAQA;QAAAb;MAAA;QACA;QACA;QACA;MACA,CAZA;IAaA,CAfA;;IAgBAc;MACA;MACA;IACA,CAnBA;;IAoBAC;MACA;MACA;MACA;IACA,CAxBA;;IAyBAC;MACA;QAAAhB;MAAA;QACA;UACA;YACAiB,WADA;YAEAC;UAFA;;UAIA;YACAC;cAAAC;YAAA;YACAC;UACA;;UACA;QACA,CAVA,MAUA;UACA;YACAJ,WADA;YAEAC;UAFA;QAIA;MACA,CAjBA;IAkBA;;EA5CA,CAjBA;EA+DAI;IACAC;MACA;QACA;UACA;YACA;;UACA;YACA;;UACA;YACA;;UACA;YACA;QARA;MAUA,CAXA;IAYA;;EAdA;AA/DA","names":["created","data","loading","users","keywords","current","size","count","isCheck","optLog","methods","listOnlineUsers","get","params","then","sizeChange","currentChange","removeOnlineUser","title","message","router","path","sessionStorage","computed","tagType"],"sourceRoot":"src/views/user","sources":["Online.vue"],"sourcesContent":["<!-- 用户管理 -->\r\n<template>\r\n  <el-card class=\"main-card\">\r\n    <div class=\"title\">{{ this.$route.name }}</div>\r\n    <div class=\"operation-container\">\r\n      <div style=\"margin-left: auto\">\r\n        <el-input\r\n          v-model=\"keywords\"\r\n          prefix-icon=\"el-icon-search\"\r\n          size=\"small\"\r\n          placeholder=\"请输入用户昵称\"\r\n          style=\"width: 200px\"\r\n          @keyup.enter.native=\"listOnlineUsers\" />\r\n        <el-button type=\"primary\" size=\"small\" icon=\"el-icon-search\" style=\"margin-left: 1rem\" @click=\"listOnlineUsers\">\r\n          搜索\r\n        </el-button>\r\n      </div>\r\n    </div>\r\n    <el-table v-loading=\"loading\" :data=\"users\">\r\n      <el-table-column type=\"selection\" width=\"55\" align=\"center\" />\r\n      <el-table-column prop=\"avatar\" label=\"头像\" align=\"center\" width=\"100\">\r\n        <template slot-scope=\"scope\">\r\n          <img :src=\"scope.row.avatar\" width=\"40\" height=\"40\" />\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column prop=\"nickname\" label=\"昵称\" align=\"center\" />\r\n      <el-table-column prop=\"ipAddress\" label=\"ip地址\" align=\"center\" />\r\n      <el-table-column prop=\"ipSource\" label=\"登录地址\" align=\"center\" width=\"200\" />\r\n      <el-table-column prop=\"browser\" label=\"浏览器\" align=\"center\" width=\"160\" />\r\n      <el-table-column prop=\"os\" label=\"操作系统\" align=\"center\" />\r\n      <el-table-column prop=\"lastLoginTime\" label=\"登录时间\" align=\"center\" width=\"200\">\r\n        <template slot-scope=\"scope\">\r\n          <i class=\"el-icon-time\" style=\"margin-right: 5px\" />\r\n          {{ scope.row.lastLoginTime | dateTime }}\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"操作\" align=\"center\" width=\"150\">\r\n        <template slot-scope=\"scope\">\r\n          <el-popconfirm title=\"确定下线吗？\" style=\"margin-left: 10px\" @confirm=\"removeOnlineUser(scope.row)\">\r\n            <el-button size=\"mini\" type=\"text\" slot=\"reference\"> <i class=\"el-icon-delete\" /> 下线 </el-button>\r\n          </el-popconfirm>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n    <el-pagination\r\n      class=\"pagination-container\"\r\n      background\r\n      @size-change=\"sizeChange\"\r\n      @current-change=\"currentChange\"\r\n      :current-page=\"current\"\r\n      :page-size=\"size\"\r\n      :total=\"count\"\r\n      :page-sizes=\"[10, 20]\"\r\n      layout=\"total, sizes, prev, pager, next, jumper\" />\r\n  </el-card>\r\n</template>\r\n\r\n<script>\r\nimport router from '@/router'\r\n\r\nexport default {\r\n  created() {\r\n    this.current = this.$store.state.pageState.online\r\n    this.listOnlineUsers()\r\n  },\r\n  data() {\r\n    return {\r\n      loading: true,\r\n      users: [],\r\n      keywords: null,\r\n      current: 1,\r\n      size: 10,\r\n      count: 0,\r\n      isCheck: false,\r\n      optLog: {}\r\n    }\r\n  },\r\n  methods: {\r\n    listOnlineUsers() {\r\n      this.axios\r\n        .get('/api/admin/users/online', {\r\n          params: {\r\n            current: this.current,\r\n            size: this.size,\r\n            keywords: this.keywords\r\n          }\r\n        })\r\n        .then(({ data }) => {\r\n          this.users = data.data.records\r\n          this.count = data.data.count\r\n          this.loading = false\r\n        })\r\n    },\r\n    sizeChange(size) {\r\n      this.size = size\r\n      this.listOnlineUsers()\r\n    },\r\n    currentChange(current) {\r\n      this.current = current\r\n      this.$store.commit('updateOnlinePageState', current)\r\n      this.listOnlineUsers()\r\n    },\r\n    removeOnlineUser(user) {\r\n      this.axios.delete('/api/admin/users/' + user.userInfoId + '/online').then(({ data }) => {\r\n        if (data.flag) {\r\n          this.$notify.success({\r\n            title: '成功',\r\n            message: data.message\r\n          })\r\n          if (user.userInfoId == this.$store.state.userInfo.id) {\r\n            router.push({ path: '/login' })\r\n            sessionStorage.removeItem('token')\r\n          }\r\n          this.listOnlineUsers()\r\n        } else {\r\n          this.$notify.error({\r\n            title: '失败',\r\n            message: data.message\r\n          })\r\n        }\r\n      })\r\n    }\r\n  },\r\n  computed: {\r\n    tagType() {\r\n      return function (type) {\r\n        switch (type) {\r\n          case 'GET':\r\n            return ''\r\n          case 'POST':\r\n            return 'success'\r\n          case 'PUT':\r\n            return 'warning'\r\n          case 'DELETE':\r\n            return 'danger'\r\n        }\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\nlabel {\r\n  font-weight: bold !important;\r\n}\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}