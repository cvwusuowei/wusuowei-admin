tTick(() => {\r\n        Prism.highlightAll()\r\n      })\r\n    }\r\n  },\r\n  computed: {\r\n    tagType() {\r\n      return function (type) {\r\n        switch (type) {\r\n          case 'GET':\r\n            return ''\r\n          case 'POST':\r\n            return 'success'\r\n          case 'PUT':\r\n            return 'warning'\r\n          case 'DELETE':\r\n            return 'danger'\r\n        }\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped></style>\r\n"],"mappings":";AAqGA;EACAA,QAAA;IACA,KAAAC,OAAA,QAAAC,MAAA,CAAAC,KAAA,CAAAC,SAAA,CAAAC,YAAA;IACA,KAAAC,QAAA;EACA;EACAC,KAAA;IACA;MACAC,IAAA;MACAC,MAAA;MACAC,QAAA;MACAC,OAAA;MACAC,QAAA;MACAX,OAAA;MACAY,IAAA;MACAC,KAAA;MACAT,YAAA;MACAU,OAAA;MACAC,aAAA;IACA;EACA;EACAC,OAAA;IACAC,gBAAAV,IAAA;MACA,KAAAC,MAAA;MACAD,IAAA,CAAAW,OAAA,CAAAC,IAAA;QACA,KAAAX,MAAA,CAAAY,IAAA,CAAAD,IAAA,CAAAE,EAAA;MACA;IACA;IACAC,WAAAV,IAAA;MACA,KAAAA,IAAA,GAAAA,IAAA;MACA,KAAAP,QAAA;IACA;IACAkB,cAAAvB,OAAA;MACA,KAAAA,OAAA,GAAAA,OAAA;MACA,KAAAC,MAAA,CAAAuB,MAAA,gCAAAxB,OAAA;MACA,KAAAK,QAAA;IACA;IACAoB,WAAA;MACA,KAAAzB,OAAA;MACA,KAAAC,MAAA,CAAAuB,MAAA,qCAAAxB,OAAA;MACA,KAAAK,QAAA;IACA;IACAA,SAAA;MACA,KAAAqB,KAAA,CACAC,GAAA;QACAC,MAAA;UACA5B,OAAA,OAAAA,OAAA;UACAY,IAAA,OAAAA,IAAA;UACAD,QAAA,OAAAA;QACA;MACA,GACAkB,IAAA;QAAAvB;MAAA;QACA,KAAAC,IAAA,GAAAD,IAAA,CAAAA,IAAA,CAAAwB,OAAA;QACA,KAAAjB,KAAA,GAAAP,IAAA,CAAAA,IAAA,CAAAO,KAAA;QACA,KAAAH,OAAA;MACA;IACA;IACAqB,UAAAV,EAAA;MACA,IAAAW,KAAA;MACA,IAAAX,EAAA;QACAW,KAAA;UAAA1B,IAAA,GAAAe,EAAA;QAAA;MACA;QACAW,KAAA;UAAA1B,IAAA,OAAAE;QAAA;MACA;MACA,KAAAkB,KAAA,CAAAO,MAAA,8BAAAD,KAAA,EAAAH,IAAA;QAAAvB;MAAA;QACA,IAAAA,IAAA,CAAA4B,IAAA;UACA,KAAAC,OAAA,CAAAC,OAAA;YACAC,KAAA;YACAC,OAAA,EAAAhC,IAAA,CAAAgC;UACA;UACA,KAAAjC,QAAA;QACA;UACA,KAAA8B,OAAA,CAAAI,KAAA;YACAF,KAAA;YACAC,OAAA,EAAAhC,IAAA,CAAAgC;UACA;QACA;QACA,KAAA7B,QAAA;MACA;IACA;IACA+B,MAAApC,YAAA;MACA,KAAAA,YAAA,GAAAA,YAAA;MACA,KAAAW,aAAA,eAAAX,YAAA,CAAAW,aAAA;MACA,KAAAD,OAAA;MACA,KAAA2B,SAAA;QACAC,KAAA,CAAAC,YAAA;MACA;IACA;EACA;