is.listUsers()\r\n        } else {\r\n          this.$notify.error({\r\n            title: '失败',\r\n            message: data.message\r\n          })\r\n        }\r\n        this.isEdit = false\r\n      })\r\n    },\r\n    listUsers() {\r\n      this.axios\r\n        .get('/api/admin/users', {\r\n          params: {\r\n            current: this.current,\r\n            size: this.size,\r\n            keywords: this.keywords,\r\n            loginType: this.loginType\r\n          }\r\n        })\r\n        .then(({ data }) => {\r\n          this.userList = data.data.records\r\n          this.count = data.data.count\r\n          this.loading = false\r\n        })\r\n    },\r\n    listRoles() {\r\n      this.axios.get('/api/admin/users/role').then(({ data }) => {\r\n        this.userRoles = data.data\r\n      })\r\n    }\r\n  },\r\n  watch: {\r\n    loginType() {\r\n      this.current = 1\r\n      this.listUsers()\r\n    }\r\n  }\r\n}\r\n</script>\r\n"],"mappings":";AAyGA;EACAA,QAAA;IACA,KAAAC,OAAA,QAAAC,MAAA,CAAAC,KAAA,CAAAC,SAAA,CAAAC,IAAA;IACA,KAAAC,SAAA;IACA,KAAAC,SAAA;EACA;EACAC,IAAA,WAAAA,CAAA;IACA;MACAC,OAAA;MACAC,MAAA;MACAC,QAAA;QACAC,UAAA;QACAC,QAAA;MACA;MACAC,SAAA;MACAC,SAAA;MACAC,OAAA;MACAC,QAAA;MACAC,QAAA,GACA;QACAC,IAAA;QACAC,IAAA;MACA,GACA;QACAD,IAAA;QACAC,IAAA;MACA,EACA;MACAC,QAAA;MACApB,OAAA;MACAqB,IAAA;MACAC,KAAA;IACA;EACA;EACAC,OAAA;IACAC,YAAA;MACA,KAAAxB,OAAA;MACA,KAAAK,SAAA;IACA;IACAoB,WAAAJ,IAAA;MACA,KAAAA,IAAA,GAAAA,IAAA;MACA,KAAAhB,SAAA;IACA;IACAqB,cAAA1B,OAAA;MACA,KAAAA,OAAA,GAAAA,OAAA;MACA,KAAAC,MAAA,CAAA0B,MAAA,wBAAA3B,OAAA;MACA,KAAAK,SAAA;IACA;IACAuB,cAAAxB,IAAA;MACA,KAAAyB,KAAA,CAAAC,GAAA;QACAC,EAAA,EAAA3B,IAAA,CAAAO,UAAA;QACAqB,SAAA,EAAA5B,IAAA,CAAA4B;MACA;IACA;IACAC,cAAA7B,IAAA;MACA,KAAAW,OAAA;MACA,KAAAL,QAAA,GAAAwB,IAAA,CAAAC,KAAA,CAAAD,IAAA,CAAAE,SAAA,CAAAhC,IAAA;MACA,KAAAM,QAAA,CAAA2B,KAAA,CAAAC,OAAA,CAAAC,IAAA;QACA,KAAAxB,OAAA,CAAAyB,IAAA,CAAAD,IAAA,CAAAR,EAAA;MACA;MACA,KAAAtB,MAAA;IACA;IACAgC,aAAA;MACA,KAAA/B,QAAA,CAAAK,OAAA,QAAAA,OAAA;